{
  "name": "@taskany/bricks",
  "version": "3.0.1",
  "description": "Taskany Design System",
  "main": "index.js",
  "module": "es/index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "index.d.js.map",
    "components",
    "context",
    "utils",
    "hooks",
    "es"
  ],
  "scripts": {
    "clean": "rm -rf ./components ./context ./es ./hooks ./utils index.d.ts index.js index.js.map",
    "build": "npm run clean && npm run build:cjm && npm run build:esm && npm run build:types",
    "build:cjm": "BABEL_ENV=cjs babel ./src --out-dir . --extensions .ts,.tsx",
    "build:esm": "BABEL_ENV=esm babel ./src --out-dir ./es --extensions .ts,.tsx",
    "build:types": "tsc --outDir . --emitDeclarationOnly",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "prepare": "husky install",
    "type-check": "npx tsc --noEmit",
    "storybook": "BABEL_ENV=dev storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.22.10",
    "@babel/core": "7.22.9",
    "@babel/plugin-transform-modules-commonjs": "7.22.5",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.11",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.6.7",
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/blocks": "7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/react-webpack5": "7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@types/md5": "2.3.2",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-plugin-inline-react-svg": "2.0.2",
    "eslint": "8.48.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.13",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "2.8.8",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.3.2",
    "styled-components": "5.3.11",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11"
  },
  "dependencies": {
    "@monaco-editor/react": "4.5.1",
    "@taskany/colors": "1.1.0",
    "@taskany/icons": "1.1.2",
    "@tippyjs/react": "4.2.6",
    "color-layer": "1.0.3",
    "md5": "2.3.0",
    "react-dropzone": "14.2.3"
  },
  "sideEffects": false
}
