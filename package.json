  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./harmony": {
      "import": "./lib/harmony/index.js"
    },
    "./harmony/style.css": "./lib/harmony/style.css"
  },
